{
  "id": "canlend-unsatisfied-ccjs",
  "title": "CanLend Unsatisfied CCJs",
  "description": "",
  "category": "",
  "rules": [
    {
      "key": "Applicant[].CreditIssue.CCJAggregated.Unsatisfiedcount",
      "expression": "=count(Applicant[].CreditIssue.CCJ[].Amount) - CountIfNotEmpty(Applicant[].CreditIssue.CCJ[].DateSatisfied)",
      "order": 1,
      "metadata": {
        "description": ""
      }
    },
    {
      "key": "Applicant[].CanLend.UnsatisfiedCCJ",
      "expression": "=(Applicant[].CreditIssue.CCJAggregated.Unsatisfiedcount==0) or (Applicant[].CreditIssue.CCJAggregated.Unsatisfiedcount>0 and AllowsUnsatisfied.CCJ)",
      "order": 2,
      "metadata": {
        "description": ""
      }
    }
  ],
  "status": "published",
  "has_draft": false,
  "branch_name": null
}